{
  "name": "pmd-vscode",
  "displayName": "pmd-vscode",
  "description": "Language tools for Professional Music Driver on VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mml",
      "aliases": ["MML", "mml"],
      "extensions": [".mml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mml",
      "scopeName": "source.mml",
      "path": "./syntaxes/mml.tmGrammar.json"
    }]
  }
}
